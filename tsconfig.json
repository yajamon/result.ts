{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "ESNext",
    "declaration": true,
    "declarationMap": true,

    "rootDir": "src",
    "outDir": "lib",

    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,

    "inlineSourceMap": true,
    "inlineSources": true,

    "types": [
    ],

    "forceConsistentCasingInFileNames": true
  },
  "include": [
      "src/**/*.ts"
  ]
}
